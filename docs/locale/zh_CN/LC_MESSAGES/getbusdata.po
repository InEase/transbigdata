#: of transbigdata.crawler.getbusdata:5
msgid "city name"
msgstr "城市名称"

#: of transbigdata.crawler.getbusdata:7
msgid "Keyword, the line name"
msgstr "关键字，行名"

#: of transbigdata.crawler.getbusdata:9
msgid "Accurate matching"
msgstr "精准匹配"

#: of transbigdata.crawler.get_isochrone_amap:16
